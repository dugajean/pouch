{
    "name": "dugajean/pouch",
    "type": "library",
    "description": "Tiny IoC Container with automatic resolution for your PHP project.",
    "keywords": ["dependency injection", "container", "ioc", "di"],
    "homepage": "http://dukagj.in",
    "license": "MIT",
    "authors": [
        {
            "name": "Dukagjin Surdulli",
            "email": "me@dukagj.in"
        }
    ],
    "require": {
        "php": "^7",
        "ext-json": "*",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.13"
    },
    "suggest": {
        "ext-apcu": "Enabling APCu will enhance the performance of Pouch in case no other cache store is available."
    },
    "autoload": {
        "psr-4": {
            "Pouch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pouch\\Tests\\": "tests/"
        }
    }
}
